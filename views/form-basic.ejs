<%- include('partials/header.ejs'); -%>

<head>

    <meta charset="UTF-8">
  <link rel='stylesheet' href='https://cdn.jsdelivr.net/foundation/5.5.0/css/foundation.css'>
  <link rel="stylesheet" href="../assets/alert/style.css">

</head>
<body>

        <div class="content-wrap">
            <div class="main">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-8 p-r-0 title-margin-right">
                            <div class="page-header">
                                <div class="page-title">
                                    <h1>Hello, <span>Welcome Here</span></h1>
                                </div>
                            </div>
                        </div>
                        
                        <div class="col-lg-12">
                            <div class="card">
                                <div class="card-title">
                                    <h4>Add New Employee</h4>

                                </div>


                                <div class="card-body">
                                    <div class="horizontal-form-elements">
                                        <form class="form-horizontal" action="\addemployee" method="post">
                                            <div class="row">
                                                <div class="col-lg-6">

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Employee Name</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" name="empName" class="form-control"
                                                                placeholder="Enter Name" required>
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Email ID</label>
                                                        <div class="col-sm-10">
                                                        <input id="example-email" name="emailID" class="form-control" type="email" placeholder="admin@pict.edu" name="example-email" required>
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">UAN</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" name="uan" class="form-control"
                                                                placeholder="UAN" required>
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Department</label>
                                                        <div class="col-sm-10">
                                                            <input type="radio" id="comp" name="dept" value="comp" required>
                                                              <label for="comp">Computer</label><br>
                                                              <input type="radio" id="IT" name="dept" value="IT" required>
                                                              <label for="IT">IT</label><br>
                                                              <input type="radio" id="entc" name="dept" value="entc" required>
                                                              <label for="entc">ENTC</label><br>
                                                              <input type="radio" id="accounts" name="dept" value="accounts" required>
                                                              <label for="entc">Accounts</label><br>
                                                              <input type="radio" id="admin" name="dept" value="admin" required>
                                                              <label for="entc">Admin</label><br>
                                                        </div>
                                                    </div>

                                                   

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Designation Category</label>
                                                        <div class="col-sm-10">
                                                            <select class="form-control" name="designationCategory">
                                                                <!-- <option value="value" selected>Option Name</option> -->
                                                                <option value="Teaching" id="Teaching" selected>Teaching</option>
                                                                <option value="Non-Teaching" id="Non-Teaching" selected>Non-Teaching</option>
                                                                <option value="Other" id="Other" selected>Other</option>
                                                               
                                                            </select>
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Designation</label>
                                                        <div class="col-sm-10">
                                                            <select class="form-control" name="designationCategory">
                                                                <!-- <option value="value" selected>Option Name</option> -->
                                                                <option value="hod" id="hod" selected>HOD</option>
                                                                <option value="teacher" id="teacher" selected>Teacher</option>
                                                                <option value="Other" id="Other" selected>Other</option>
                                                               
                                                            </select>
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Salary Category</label>
                                                        <div class="col-sm-10">
                                                            <select class="form-control" name="salaryCategory">
                                                                <!-- <option value="value" selected>Option Name</option> -->
                                                                <option value="consolidated" id="consolidated" selected>
                                                                    Consolidated</option>
                                                                <option value="payscale" id="payscale" selected>Pay
                                                                    Scale</option>

                                                            </select>
                                                        </div>

                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Date of
                                                            Joining</label>
                                                        <div class="col-sm-10">
                                                            <input type="date" class="form-control" id="doj"
                                                                name="doj" required>

                                                        </div>
                                                    </div>
                                                </div>
                                                    <!-- /# column -->
                                                    <!-- <div class="column"> -->
                                                    <div class="col-lg-6">

                                                        

                                                        <div class="form-group">
                                                            <label class="col-sm-6 control-label">Bank Name</label>
                                                            <div class="col-sm-10">
                                                                <input type="text" name="bankName" class="form-control"
                                                                    placeholder="Bank Name" required>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="col-sm-6 control-label">Branch Name</label>
                                                            <div class="col-sm-10">
                                                                <input type="text" name="branchName" class="form-control"
                                                                    placeholder="Branch Name" required>

                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="col-sm-6 control-label">IFSC Code</label>
                                                            <div class="col-sm-10">
                                                                <input type="text" name="ifscCode" class="form-control"
                                                                    placeholder="IFSC Code" required>

                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="col-sm-6 control-label">Enter Account
                                                                No.</label>
                                                            <div class="col-sm-10">
                                                                <input type="text" name="bankAccNum"
                                                                    class="form-control" placeholder="Acc No." required>

                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="col-sm-6 control-label">Basic Pay</label>
                                                            <div class="col-sm-10">
                                                                <input type="text" name="pay" class="form-control"
                                                                    placeholder="Basic Pay" required>


                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="col-sm-6 control-label">Grade Pay</label>
                                                            <div class="col-sm-10">
                                                                <input type="text" name="gp" class="form-control"
                                                                    placeholder="Grade Pay" required>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="col-sm-6 control-label">Provident Fund</label>
                                                            <div class="col-sm-10">
                                                                <input type="text" name="pf" class="form-control"
                                                                    placeholder="Provident Fund" required>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="col-sm-6 control-label">Group Insurance</label>
                                                            <div class="col-sm-10">
                                                                <input type="text" name="groupInsurance" class="form-control"
                                                                    placeholder="Group Insurance" required>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="col-sm-6 control-label">Pay Band</label>
                                                            <div class="col-sm-10">
                                                                <select class="form-control" name="payBand" required>
                                                                    <!-- <option value="value" selected>Option Name</option> -->
                                                                    <option value="20k-30k" id="20k-30k" selected>20k-30k</option>
                                                                    <option value="30k-40k" id="30k-40k" selected>30k-40k</option>
                                                                    <option value="40k-50k" id="40k-50k" selected>40k-50k</option>
                                                                    <option value="50k-60k" id="50k-60k" selected>50k-60k</option>
                                                                    <option value="60k-70k" id="60k-70k" selected>60k-70k</option>
                                                                    <option value="70k-80k" id="70k-80k" selected>70k-80k</option>
                                                                    <option value="80k-90k" id="80k-90k" selected>80k-90k</option> 
                                                                    <option value="90k-100k" id="90k-100k" selected>90k-100k</option>  
                                                                    <option value="lakh" id="lakh" selected>1 lakh+</option>  
                                                                </select>
                                                            </div>
                                                        </div>
    
                                                        <!-- <button type="submit" onclick="alertFunction()" class="btn btn-primary">Primary</button> -->
                                                    <!-- <input type="submit" value="Submit"> -->



                                                    <!-- <div class="alert alert-warning alert-dismissible fade show" role="alert" value="submit" id="myAlert">
                                                        <strong>Holy guacamole!</strong> You should check in on some of those fields below.
                                                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                          <span aria-hidden="true">&times;</span>
                                                        </button>


                                                      </div> -->

                                                      <!-- <div class='customAlert'>
                                                        <p class='message'></p>
                                                          <input type='button' class='confirmButton' value='Ok'>
                                                      </div>
                                                        
                                                      <input type='button' class='rab' value='Raise alert'> -->

                                                      <!-- <button type="submit" value="showAlert" onclick="showAlert();"> Show Alert</button>

                                                        <div class="container" style="display:none;" id="myAlert">
                                                            <h2>Dismissal Alert Messages</h2>
                                                            <div class="alert alert-success alert-dismissable" id="myAlert2">
                                                                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                                                                Success! message sent successfully.
                                                            </div>

                                                        </div> -->

                                                        
                                                        <br />
                                                        <div class="row text-center">
                                                          <div>
                                                            <button class="button large round success">Submit</button>
                                                          </div>
                                                          
                                                        </div>
                                                    </div>

                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    </section>
                </div>
            </div>
        </div>

        <script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
        <script src='https://cdnjs.cloudflare.com/ajax/libs/foundation/5.5.0/js/foundation.min.js'></script><script  src="../assets/alert/script.js"></script>
</body>