                        <div class="col-lg-12">
                            <div class="card">
                                <div class="card-title">
                                    <h4>Add New Employee</h4>
                                    
                                </div>

                                
                                <div class="card-body">
                                    <div class="horizontal-form-elements">
                                        <form class="form-horizontal" action="\form-basic" method="post">
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Enter Employee ID</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" name="id" class="form-control" placeholder="Enter ID">
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Employee Name</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" name="fname" class="form-control" placeholder="Enter Name">
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">UAN</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" name="uan" class="form-control" placeholder="UAN">
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Department</label>
                                                        <div class="col-sm-10">   
                                                            <input type="radio" id="comp" name="dept" value="comp">
                                                            <label for="comp">Computer</label><br>
                                                            <input type="radio" id="IT" name="dept" value="IT">
                                                            <label for="IT">IT</label><br>
                                                            <input type="radio" id="entc" name="dept" value="entc">
                                                            <label for="entc">ENTC</label>
                                                        </div> 
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Designation</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" name="designation" class="form-control" placeholder="Designation">
                                                        </div>
                                                    </div>

                                    
                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Contact Details</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" name="contact" class="form-control" placeholder="Enter Contact No.">
                                                        </div>
                                                    </div>


                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Date of Joining</label>
                                                        <div class="col-sm-10">
                                                            <input type="date" class="form-control" id="doj" name="doj">
                                                            
                                                        </div>
                                                    </div>
                                                   
                                                </div>
                                                <!-- /# column -->
                                                <div class="col-lg-6">
                                                    
                                                    
                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Bank Name</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" name="bankname" class="form-control" placeholder="Bank Name">
                                                            
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Enter Account No.</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" name="accno" class="form-control" placeholder="Acc No.">
                                                            
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Basic Pay</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" name="basicpay" class="form-control" placeholder="Basic Pay">

                                                            
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="col-sm-6 control-label">Grade Pay</label>
                                                        <div class="col-sm-10">
                                                            <input type="text" name="gradepay" class="form-control" placeholder="Grade Pay">   
                                                        </div>
                                                    </div>

                                                    
                                               
                                                </div>
                                            <input type="submit" value="Submit">
                                                
                                            </div>

